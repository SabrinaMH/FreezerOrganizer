<Window x:Class="FreezerOrganizer.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
        xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
        xmlns:view="clr-namespace:FreezerOrganizer.View"
        xmlns:viewModel="clr-namespace:FreezerOrganizer.ViewModel"
        Title="Freezer Organizer">
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Loaded">
            <ei:CallMethodAction TargetObject="{Binding}" MethodName="Load"/>
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <Grid>
        <view:SearchView/>
    </Grid>
</Window>
